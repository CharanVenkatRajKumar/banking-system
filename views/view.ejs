<%- include("partials/header") %>
<div>
    <h1 class="view-header"> USERS LIST </h1>
</div>

<div class='table'>
    
      
       
        <table  style="color:rgb(0, 0, 0); font-weight: 1000;" >
            
            <tr>
                <th>Customer Name</th>
                <th>Email</th>
                <th>Money</th>
                <th></th>
            </tr>
            <% for(let user of users){ %>
                <tr>
                    <td><%= user.name %></td>
                    <td><%= user.email%></td>
                    <td><%= user.credits%></td>
                    <td><button class="select-btn"><a href="/view/<%= user._id %>">Pay</a></button></td>
                </tr>
            <% } %>
        </table>
</div>
<aside>
    <img src="/images/image1.jpg" alt="">
</aside>


<%- include("partials/footer") %>
